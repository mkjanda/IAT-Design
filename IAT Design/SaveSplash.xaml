<Window x:Class="IATClient.SaveSplash"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:IATClient"
             mc:Ignorable="d" 
             Height="200" Width="500" WindowStyle="None" AllowsTransparency="True">
    <Window.Resources>
        <ResourceDictionary Source="SplashDictionary.xaml" />
    </Window.Resources>
    <Window.Background>
        <ImageBrush ImageSource="{StaticResource Splash}" Opacity=".65" Stretch="Uniform" />
    </Window.Background>
</Window>
