<Window x:Class="Launcher.DownloadSplash"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="IAT Design" Height="175" Width="500" WindowStyle="None" AllowsTransparency="True" IsHitTestVisible="True">
    <Window.Resources>
        <ResourceDictionary Source="Dictionary.xaml" />
    </Window.Resources>
    <Window.Background>
        <SolidColorBrush />
    </Window.Background>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto" />
        </Grid.RowDefinitions>
        <Image Grid.Row="0"  Source="{StaticResource Logo}"/>
    </Grid>
</Window>
